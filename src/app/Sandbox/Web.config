<configuration>
  <system.web>
    <compilation debug="false" targetFramework="4.0" />
  </system.web>
  <system.webServer>
    <modules>
      <add name="CrosswalkModule" />
    </modules>
    <handlers>
      <clear/>
      <add name="Sandbox" path="*" verb="*" modules="CrosswalkModule" type="Crosswalk.Gate.Binder, Crosswalk.Gate" scriptProcessor="Sandbox.Startup" />
    </handlers>
  </system.webServer>
</configuration>
